<UserControl x:Class="ToolbarModule.Views.FileDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ToolbarModule.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="{Binding Message}"/>
        <Button Content="OK" Command="{Binding OkCommand}" CommandParameter="ok"/>
        <Button Content="Cancel" Command="{Binding CancelCommand}" CommandParameter="Cancel"/>
    </StackPanel>
</UserControl>
